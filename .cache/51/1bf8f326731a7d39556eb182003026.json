ce":"src/components/LoginForm.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":87,"column":17}},{"source":"src/components/LoginForm.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":87,"column":18}},{"source":"src/components/LoginForm.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":87,"column":23}},{"source":"src/components/LoginForm.vue","name":null,"original":{"line":82,"column":0},"generated":{"line":88,"column":0}},{"source":"src/components/LoginForm.vue","name":null,"original":{"line":38,"column":0},"generated":{"line":89,"column":0}},{"source":"src/components/LoginForm.vue","name":null,"original":{"line":28,"column":0},"generated":{"line":90,"column":0}}],"sources":{"src/components/LoginForm.vue":"<template>\n    <div class=\"row mt-5\">\n        <div class=\"col-6 mx-auto\">\n            <h3 class=\"h3\">Login</h3>\n            <form @submit.prevent=\"login\">\n                <div class=\"form-group\">\n                    <label for=\"exampleInputEmail1\">Email address</label>\n                    <input type=\"email\" class=\"form-control\" id=\"exampleInputEmail1\" aria-describedby=\"emailHelp\" v-model=\"email\">\n                    <small id=\"emailHelp\" class=\"form-text text-muted\">We'll never share your email with anyone else.</small>\n                </div>\n                <div class=\"form-group\">\n                    <label for=\"exampleInputPassword1\">Password</label>\n                    <input type=\"password\" class=\"form-control\" id=\"exampleInputPassword1\" v-model=\"password\">\n                </div>\n                <button type=\"submit\" class=\"btn btn-primary\">Login</button>\n            </form><br>\n            <div class=\"btn-group\">\n                <button v-google-signin-button=\"clientId\" class=\"google-signin-button\"> Continue with Google</button>\n                <button class=\"btn btn-link\" @click=\"toRegister\">Register</button>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from '../config/axios'\n\nexport default {\n    name: 'LoginForm',\n    data() {\n        return {\n            email: '',\n            password: '',\n            clientId: '878537891560-p61p1ef06m8u0slvapa7757e8qdpf8tb.apps.googleusercontent.com'\n        }     \n    },\n\n    methods: {\n        login(){\n            axios({\n                url: \"/login\",\n                method: \"post\",\n                data: {\n                email: this.email,\n                password: this.password\n                }\n            })\n            .then(user=>{\n                localStorage.setItem(\"token\", user.data.token)\n                localStorage.setItem(\"email\", user.data.email)\n                this.$emit('changePage', 'main-page')\n            })\n            .catch(err => {\n                console.log(err)\n            })\n        },\n        toRegister(){\n            this.$emit('changePage', 'register-page')\n        },\n\n        OnGoogleAuthSuccess (idToken) {\n          \n            axios({\n                url: `/googleSignIn`,\n                method: \"post\",\n                headers: {\n                    id_token: idToken\n                }\n            })\n            .then(user => {\n                localStorage.setItem(\"token\", user.data.token)\n                localStorage.setItem('email', user.data.email)\n                this.$emit('changePage', 'main-page')\n            })\n            .catch(err =>{\n                console.log(err)\n            })\n\n        },\n        OnGoogleAuthFail (error) {\n            console.log(error)\n        }\n    }\n}\n</script>\n\n<style>\n    .google-signin-button {\n  color: white;\n  background-color: darkblue;\n  height: 50px;\n  font-size: 16px;\n  border-radius: 10px;\n  padding: 10px 20px 25px 20px;\n  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\n}\n\nh3{\n    color: black;\n}\n\n</style>"},"lineCount":null}},"error":null,"hash":"69737e9529d13c14be2926d67b54f1dc","cacheData":{"env":{}}}